<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<c:set var="ctx" value="${pageContext.request.contextPath}"/>
<!DOCTYPE html>
<html>
<head>
<script type="text/javascript" src="${ctx}/static/js/jquery-1.9.1.js"></script>
<script type="text/javascript" src="static/js/jquery.form.js"></script>
<link rel="stylesheet" href="${ctx}/static/js/kindeditor/themes/default/default.css" />
<script type="text/javascript" charset="utf-8" src="${ctx}/static/js/kindeditor/kindeditor.js"></script>  
<script type="text/javascript" charset="utf-8" src="${ctx}/static/js/kindeditor/lang/zh_CN.js"></script> 
</head>
<body>
	<form id="frm" name="frm" method="post" enctype="multipart/form-data" action="" onsubmit="return false;">
		<div style="margin:2%;">
			<input id="id" name="id" type="hidden" value='${vo.id}'/>
			<input type="hidden" name="type" id="type" value="0">
			
			<div class="tanchu">
				<ul class="fl">
				<div><img src="data:image/png;base64," alt="" />
				</div>
					<li>
						<p>商品名称：</p>
						<input type="text" style="width:300px;" id="name" name="name" value="${vo.name}" placeholder="请输商品名称" />
						<span style="color: #ff0000">&nbsp;*</span>
					</li>
					<li>
						<p>商品编号：</p>
						<input type="text" style="width:300px;" id="number" name="number" value="${vo.number}" placeholder="请输商品编号" />
						<span style="color: #ff0000">&nbsp;*</span>
					</li>
					<li>
						<p>商品类别：</p>
						<select id="categoryId1" name="categoryId1" style="width:145px;" onchange="chooseCategory(this.value);">
							<option value="">-商品大类-</option>
							
						</select>
						<select id="categoryId2" name="categoryId2" style="width:145px;">
							
						</select>
						<span style="color:#ff0000">*</span>
					</li>
				</ul>
			    <div class="clear"></div>
			        图文详情：
			  <textarea name="content" class="common-textarea" id="content" style="width:700px; height:400px;visibility:hidden;"></textarea>
				<div class="bu">
					<input type="button" onclick="save();" value="保存" class="get" id="subSave"/>
				</div>
				<input type="button" name="getHtml" value="取得HTML" />
				<input type="button" name="getText" value="取得文本(包含img,embed)" />
			</div>
		</div>
	</form>
</body>
<script type="text/javascript">
 var ctx = "${ctx}";
 var editor;
 KindEditor.ready(function(K){
     editor = K.create("textarea[id='content']",{
         resizeType : 1,
         allowPreviewEmoticons: false,
         allowImageUpload:true,//允许上传图片
         allowFileManager:true, //允许对上传图片进行管理
         allowImageRemote : true, //允许上传网络图片
       //uploadJson: ctx + '/static/js/kindeditor/jsp/upload_json.jsp', //上传图片的java代码，只不过放在jsp中
         uploadJson: ctx + '/uploadFile/upload2', //上传图片的java代码，只不过放在jsp中
         //fileManagerJson: ctx + '/static/js/kindeditor/jsp/file_manager_json.jsp',
         afterUpload: function(){
        	 this.sync();
        /* 	 var a = $("#uppics").val();
             if (a.length > 0) {  
                 $("#uppics").val(a + "," + url);  
             } else {  
                 $("#uppics").val(url);  
             } */
         }, //图片上传后，将上传内容同步到textarea中
         afterCreate: function () {  //要取值设置这里 这个函数就是同步KindEditor的值到textarea文本框  
             this.sync();  
         },  
         afterBlur: function(){this.sync();},   ////失去焦点时，将上传内容同步到textarea中
        /*  items : [
             'fontname','fontsize', '|','forecolor', 'hilitecolor','bold', 'italic','underline',
             'removeformat','|', 'justifyleft','justifycenter', 'justifyright','insertorderedlist',
             'insertunorderedlist','|', 'emoticons','link','media','|','image','multiimage']  */
     });
     K('input[name=getHtml]').click(function(e) {
			alert(editor.html());
		});
		K('input[name=getText]').click(function(e) {
			alert(editor.text());
		});

 });

/**
 * 保存
 */
function save() {
	
//	alert($("#content").val());
	//$("button").attr("disabled","disabled");
	
		$("#frm").ajaxSubmit({
			type : 'post',
			url : ctx + "/uploadFile/upload2",
			success : function(responseText, statusText) {
					alert(0);
			},error : function() {
				alert('操作失败!');
			//	$("button").removeAttr("disabled");
			}
		});
}

/**
 * 验证表单
 */
function validate() {
	var flag = false;
	if ($.trim($("#name").val()) == "") {
		alert("请输入商品名称！");
		$("#name").focus();
	} else if ($.trim($("#number").val()) == "") {
		alert("请输入商品编号！");
		$("#number").focus();
	} else if (checkNumber()) {
		alert("商品编号重复！");
		$("#number").focus();
	} else if ($.trim($("#categoryId1").val()) == "") {
		alert("请选择商品大类！");
	} else if (!checkMoney($("#price").val())) {
		alert("请输入正确的商品价格！");
	}else if($("#warnStock").val()==""){
		$("#warnStock").focus();
		alert("预警库存不能为空！");
	}else if(!isPosNumber($("#warnStock").val())){
		$("#warnStock").focus();
		alert("预警库存必须是大于零的正整数！");
	} else {
		flag = true;
	}
	return flag;
}

/**
 * 验证编号唯一性
 */
function checkNumber() {
	var flag = false;
	var id = $("#id").val();
	var number = $("#number").val();
	$.ajax({
		url : ctx + "/goods/checkNumber?id="+id+"&number="+number,
		type : "post",
		async : false,
		success : function(data) {
			// 有重复
			if (data == 1) {
				flag = true;
			}
		}
	});
	return flag;
}

/**
 * 获取产品小类
 */
function chooseCategory(id) {
	$("#categoryId2").html('<option value="">-商品小类-</option>');
	if ($("#categoryId1").val()!="") {
		$.ajax({
			url : ctx + "/goodsCategory/queryListById?id="+id,
			type : "post",
			async : false,
			success : function(data) {
				$.each(data,function(i,d){
					$("#categoryId2").append('<option value="'+d.id+'">'+d.name+'</option>');
				});
			}
		});
	}
}
</script>
</html>